    "version": "0.2.0",
    "configurations": [
        {
            "name": "diffuser_single",
            "type": "python",
            "request": "launch",
            "program": "src/diffusers_training_clip_sd.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--pretrained_model_name_or_path",
                "CompVis/stable-diffusion-v1-4",
                "--instance_data_dir",
                "./data/cat",
