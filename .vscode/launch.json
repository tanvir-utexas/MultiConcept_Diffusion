                "--instance_prompt",
                "photo of a <new1> cat",
                "--class_prompt",
                "cat", 
                "--resolution",
                "512",
                "--train_batch_size",
                "1",
                "--learning_rate",
                "1e-5",
                "--lr_warmup_steps",
                "0",
                "--max_train_steps",
                "250",
                "--num_class_images",
                "200",
                "--scale_lr",
                "--hflip",
                "--modifier_token",
                "<new1>"
            ]
        },
        {
            "name": "finetune_real",
            "type": "python",
            "request": "launch",
            "program": "train_debug.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--base",
                "configs/custom-diffusion/finetune_addtoken.yaml",
                "-t",
                "--gpus",
                "1",
                "finetune_addtoken.yaml",
                "--resume-from-checkpoint-custom",
                "weights/sd-v1-4.ckpt",
                "--caption",
                "<new1> cat",
                "--datapath",
                "data/cat",
                "--reg_datapath",
                "real_reg/samples_cat/images.txt",
                "--reg_caption",
                "real_reg/samples_cat/caption.txt",
                "--modifier_token",
                "<new1>",
                "--name",
                "cat-sdv4"
            ]
        }
    ]
}
